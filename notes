git config --global user.name "Name"
git config --global user.email "email"
git config --list
git config user.name
git init  
git add .
git commit -m "first commit"
git remote add origin https://github.com/kannanaikal/gittutorial.git
git push -u origin master
git status
git add <filename> to add a file
git status -s
git diff --staged
git commit -a -m  "another commit"
git log
git log  -1
git log --oneline
git log --stat
git log --patch
git rm <filename>
git rm --cached <filename> file is not deleted from filesystem
git mv oldname newname 
git checkout -b <branch_name>


